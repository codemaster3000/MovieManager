  <table-filter match-name="season"/>
  <table-filter match-name="edition"/>
  <table-filter match-name="movie_collection"/>
  <table-filter match-name="tmdbinfo"/>
  <table-filter match-name="season_has_audioline"/>
  <table-filter match-name="videoline"/>
  <table-filter match-name="tvshow"/>
  <table-filter match-name="genre"/>
  <table-filter match-name="audioline"/>
  <table-filter match-name="owner"/>
  <table-filter match-name="movie"/>
  <table-filter match-name="movie_has_genre"/>
  <table-filter match-name="movie_has_audioline"/>
